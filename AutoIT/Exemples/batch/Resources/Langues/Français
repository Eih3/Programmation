[commandes]
md=md [NOM_DU_REPERTOIRE]
rd=rd [NOM_DU_REPERTOIRE]
ren=ren [ANCIEN_NOM] [NOUVEAU_NOM]
move=move [ANCIEN_EMPLACEMENT] [NOUVEL_EMPLACEMENT]
del=del [NOM_DU_FICHIER]
type=type [FICHIER]
type2=type [FICHIER]>lpt1 REM lpt1 est le port parallele n°1
copy=copy [EMPLACEMENT_DU_FICHIER_SOURCE] [EMPLACEMENT_DU_FICHIER_COPIE]
dirfiltre=dir *.* REM Remplacez *.* par votre filtre
copy2=copy /b fichier1.txt + fichier2.txt nouveau.txt>nul
echo=echo [Données]>>[FICHIER]
echo2=echo [Données]>[FICHIER]
commande=COMMANDE>>[FICHIER]
commande2=COMMANDE>[FICHIER]
call=call [FICHIER]
fichier=[FICHIER]
start=start [FICHIER]
cd=cd [CHEMIN]
format=format [LECTEUR]
exlecteur=if exist [LECTEUR]:\NUL echo Le lecteur existe.
attrib=Attrib +r [FICHIER]
attrib2=Attrib -r [FICHIER]
attrib3=Attrib +h [FICHIER]
attrib4=Attrib -h [FICHIER]
attrib5=Attrib +a [FICHIER]
attrib6=Attrib -a [FICHIER]
cond1=if "%VARIABLE%"=="VALEUR" [COMMANDE]
cond2=if not "%VARIABLE"%=="VALEUR" [COMMANDE]
cond3=if exist "FICHIER" [COMMANDE]
cond4=if not exist "FICHIER" [COMMANDE]
cond5=if errorlevel 255 Goto LABEL REM Remplacez 255 par la bonne valeur.
cond6_1=if "%Variable%"=="Valeur" (
cond6_2=	REM - Entrez ici le code si la condition est vraie
cond6_3=	REM - Vous pouvez entrer du code sur plusieures lignes
cond6_4=	REM - Entrez ici le code si la condition est fausse
cond6_5=	REM - Vous pouvez entrer du code sur plusieures lignes
cond7=if defined Variable_environnement [Commande]
netview=net view [\\POSTE] REM Exemple : net view \\S220_M
netuse_1=net use [Disque:] [\\POSTE\DOMAINE]
netuse_2=REM [Disque:] représente une lettre de A à Z suivie de ':' correspondant au disque local à associer à la ressource réseau.
netuse_3=REM [Disque:] peut-être remplacé par '*' pour désigner le prochain identificateur de disque disponible sur votre poste.
netuse_4=REM Exemple : net use * \\P220_M\c$
deletenet_1=net use [disque:] /DELETE
deletenet_2=REM Exemple : net use f: /DELETE
ping=ping [domaine]
stratexplorer=REM P. ex. start explorer "http://www.lecoindaide.com/
ftp_1=ftp -n [Adresse FTP]
ftp_2=REM P. ex. "ftp -n ftp.monftp.com"
path=%PATH%=%PATH%;[CHEMIN_A_AJOUTER]
temp=choice /t:o,5>nul   REM Remplacez "5" par le délai en secondes de votre choix
color_1=REM - Le premier chiffre correspond à la couleur de fond, et le second à celui de premier plan :
color_2=REM 8 = Gris, 9 = Bleu, A = Vert, B = Cyan, C = Rouge, D = Rose, E = Jaune, F = Blanc 
color_3=REM 0 = Noir, 1 = Bleu foncé, 2 = Vert, 3 = Bleu-gris, 4 = Marron, 5 = Pourpre, 6 = Kaki, 7 = Gris clair
title=title [Votre titre]


